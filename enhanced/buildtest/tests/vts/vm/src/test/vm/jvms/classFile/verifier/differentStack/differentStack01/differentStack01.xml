  <Source name="differentStack01n.j"/>

  <Runner ID="Runtime">
    <Param name="toRun" value="org.apache.harmony.vts.test.vm.jvms.share.vmVerifierRun">
			<Option name="run"/>
			<Option name="org.apache.harmony.vts.test.vm.jvms.classFile.verifier.differentStack.differentStack01.differentStack01n"/>
			<Option name="java.lang.VerifyError"/>
	</Param>
  </Runner>
</Test>
