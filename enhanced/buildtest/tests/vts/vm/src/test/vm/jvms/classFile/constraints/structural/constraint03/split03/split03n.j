.class public org/apache/harmony/vts/test/vm/jvms/classFile/constraints/structural/constraint03/split03/split03n
.super java/lang/Object

;
; standard initializer
.method public <init>()V
   aload_0
   invokespecial java/lang/Object/<init>()V
   return
.end method

;
; test method
.method test()V
   .limit stack 2
   .limit locals 4

   dconst_1
   dstore_2 ; store double 1 to the local variables 2 and 3
   sipush 1
   istore_3 ; store int 1 to the local variable 3
   dload_2 ; try to load double from local variables 2 and 3

   return
.end method
