<Test
  ID="accessFlags10"
  date-of-creation="2006-05-12"
  timeout="1"
  >
  <Author value="Khen G. Kim"/>
  <Description>
    Checks that VM loads class implementing valid interface with ACC_INTERFACE and 
    ACC_ABSTRACT flags set. Positive test.    
    Covered assertions assert_class050
  </Description>

  <Keyword name="verifier"/>

  <Keyword name="pass1"/>

  <Keyword name="classloader"/>

  <Source name="accessFlags10p.j"/>
  <Source name="accessFlags10interface.j"/>

  <Runner ID="Runtime">
    <Param name="toRun" value="org.apache.harmony.vts.test.vm.jvms.share.vmVerifierRun">
                <Option name="load"/>
                <Option name="org.apache.harmony.vts.test.vm.jvms.classFile.structure.accessFlags.accessFlags10.accessFlags10p"/>
            </Param>
  </Runner>
</Test>
