<!DOCTYPE Test SYSTEM '../../../../../../../../../../test.dtd'>
<!-- 
-->


<Test
    ID="StoreTest"
    date-of-creation="2005-03-21"
    timeout="1"
    >

    
    
    
    <MulticaseTestDescription>
        <TestedClass name="java.util.Properties"/>

        <Case ID="testSetParent">
            <TestedMethod name="public void store(OutputStream out,
                  String comments)
           throws IOException"/>
            <Description>
                Store a Properties to a special stream which compares its input to 
                the Reference VM sample.
            </Description>
        </Case>
        
    </MulticaseTestDescription>

    <Keyword name="functional"/>    
    
    <Source name="StoreTest.java"/>
    
    <Restriction name="OtherVMOnly" />

    <Runner ID="MultiCase">
        <Param name="toRun" value="org.apache.harmony.test.func.api.java.util.Properties.StoreTest">
            <Option name="-propertiesPath" value="$TestDirectory/auxiliary/StoreTest.properties" />
        </Param>
    </Runner>
</Test> 
