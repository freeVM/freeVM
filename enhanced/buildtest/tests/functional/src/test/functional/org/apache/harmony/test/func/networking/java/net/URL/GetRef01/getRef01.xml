<Test
    ID="getRef01"
    date-of-creation="2004-11-16"
    timeout="1"
    >    
        
        
        <APITestDescription>
            <TestedClass name="java.net.URL"/>
            <TestedMethod name="public String getRef()"/>
            
            <Description>
                        creates URL using random value of File part (that can include or don't include Ref part)
                        and tests if this File part is equals to the concatination two strings:
                        getFile() method result and getRef() method result (null should be replaced with the empty string).
            </Description>
            </APITestDescription>
        <Keyword name="functional"/>    

        <Source name="getRef01.java"/>
            
        <Runner ID="Runtime">
            <Param name="toRun" value="org.apache.harmony.test.func.networking.java.net.URL.GetRef01.getRef01"/>
        </Runner>
</Test>
