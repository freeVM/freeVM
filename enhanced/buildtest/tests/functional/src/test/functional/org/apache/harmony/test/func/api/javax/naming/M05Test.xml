<!DOCTYPE Test SYSTEM '../../../../../../../../../test.dtd'>

<!--
-->
<Test ID="M05Test" date-of-creation="2005-08-26" timeout="1">

    

    

    <MulticaseTestDescription>
        <Case ID="testNameNotFoundException">
            <TestedClass name="javax.naming.NameNotFoundException" />
            <TestedMethod name="public NameNotFoundException()" />
            <Description></Description>
        </Case>

        <Case ID="testNamingException">
            <TestedClass name="javax.naming.NamingException" />
            <TestedMethod name="public NamingException()" />
            <Description></Description>
        </Case>

        <Case ID="testNamingExceptionString">
            <TestedClass name="javax.naming.NamingException" />
            <TestedMethod name="public NamingException(String)" />
            <Description></Description>
        </Case>

        <Case ID="testReferenceString">
            <TestedClass name="javax.naming.Reference" />
            <TestedMethod name="public Reference(String)" />
            <Description></Description>
        </Case>

        <Case ID="testReferenceStringStringString">
            <TestedClass name="javax.naming.Reference" />
            <TestedMethod
                name="public Reference(String, String, String)" />
            <Description></Description>
        </Case>

        <Case ID="testGetStateToBind">
            <TestedClass name="javax.naming.spi.NamingManager" />
            <TestedMethod
                name="public Object getStateToBind (Object, Name, Context, Hashtable&lt;?, ?&gt;)" />
            <Description></Description>
        </Case>
    </MulticaseTestDescription>

    <Keyword name="functional" />

    <Source name="M05Test.java" />

    <Runner ID="MultiCase">
        <Param name="toRun"
            value="org.apache.harmony.test.func.api.javax.naming.M05Test" />
    </Runner>
</Test>





