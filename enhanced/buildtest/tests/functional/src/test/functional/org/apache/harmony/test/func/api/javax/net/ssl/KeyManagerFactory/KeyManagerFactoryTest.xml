<!DOCTYPE Test SYSTEM '../../../../../../../../../../../test.dtd'>
<!-- 
-->


<Test
    ID="KeyManagerFactoryTest"
    date-of-creation="2005-08-02"
    timeout="1"
    >

    
    
    
    <MulticaseTestDescription>
        <TestedClass name="javax.net.ssl.KeyManagerFactory"/>

        <Case ID="testConstructor">
            <TestedMethod name="public static final String getDefaultAlgorithm()"/>
            
            <Precondition></Precondition>
            <Description>check that getDefaultAlgorithm() returns value of security property "ssl.KeyManagerFactory.algorithm".
            Then change value of this security proprty to a random value and check again
            </Description>
            <Expected>getDefaultAlgorithm() always returns value of this property</Expected>
        </Case>
    </MulticaseTestDescription>
    
    <Keyword name="functional"/>
    <Source name="KeyManagerFactoryTest.java"/>

    <Runner ID="MultiCase">
        <Param name="toRun" value="org.apache.harmony.test.func.api.javax.net.ssl.KeyManagerFactory.KeyManagerFactoryTest">
        </Param>
    </Runner>
    <Restriction name="OtherVMOnly"/>
</Test> 
