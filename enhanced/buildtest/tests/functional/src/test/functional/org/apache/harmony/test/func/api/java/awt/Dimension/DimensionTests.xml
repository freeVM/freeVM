<!DOCTYPE Test SYSTEM '../../../../../../../../../../test.dtd'>

<!--
-->

<Test
    ID="DimensionTests"
    date-of-creation="2005-05-24"
    timeout="1"
    >

    
    

    <APITestDescription>
        <TestedClass name="java.awt.Dimension"/>

        <Case ID="test">
            <TestedMethod name="public boolean equals(Object obj)"/>
            <TestedMethod name="public double getHeight()"/>
            <TestedMethod name="public Dimension getSize()"/>
            <TestedMethod name="public double getWidth()"/>
            <TestedMethod name="public int hashCode()"/>
            <TestedMethod name="public void setSize(Dimension d)"/>
            <TestedMethod name="public void setSize(double width, double height)"/>
            <TestedMethod name="public void setSize(int width, int height)"/>
            <TestedMethod name="public String toString()"/>


            <Description>A few objects of class Dimension are created by variouse creation type
                         And its properties compared by variouse methods. </Description>
            <Expected> Properties of identical objects are identical</Expected>


        </Case>

    </APITestDescription>

    <Keyword name="functional"/>
    <!--    <Keyword name = "gui"/>  -->


    <Source name="DimensionTests.java"/>

    <Runner ID="Runtime">
        <Param name="toRun" value="org.apache.harmony.test.func.api.java.awt.Dimension.DimensionTests">
        </Param>
    </Runner>
</Test>
